{
  "name": "nestjs-app",
  "version": "1.0.0",
  "description": "Spring Boot-like Node.js app using NestJS",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "clean": "rm -rf dist",
    "build": "npm run clean && nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "migration:generate": "npx ts-node ./node_modules/typeorm/cli.js migration:generate -d ./src/data-source.ts src/migrations/InitSchema",
    "migration:run": "npx ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/data-source.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.2",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/platform-express": "^11.1.2",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "class-validator": "^0.14.2",
    "dotenv": "^16.5.0",
    "pg": "^8.16.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.24"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.2",
    "@types/node": "^22.15.27",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "eslint": "^9.27.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
